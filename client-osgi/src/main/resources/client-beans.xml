<?xml version="1.0" encoding="UTF-8"?>


<beans xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns="http://www.springframework.org/schema/beans"> 
    
    
    
    <bean class="org.springframework.remoting.caucho.HessianProxyFactoryBean" id="osgiFacade">
        <property name="serviceUrl" value="http://localhost:8080/facade-executor/facadeexecutor.hessian" />
        <property name="serviceInterface" value="net.griddynamics.facade.executor.api.FacadeExecutor"/>
    </bean>
        
</beans>
